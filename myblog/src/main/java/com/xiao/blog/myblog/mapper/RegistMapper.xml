<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiao.blog.myblog.mapper.RegistMapper">

  <insert id="registUser" parameterType="com.xiao.blog.myblog.entity.User">
    insert into user(uid,phone,username,password,email,role_id) values(#{uid},#{phone},#{username},#{password},#{email},#{role_id})
  </insert>
</mapper>